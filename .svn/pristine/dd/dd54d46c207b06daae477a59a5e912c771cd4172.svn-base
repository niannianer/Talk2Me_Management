@charset "UTF-8";
@import "../../Common/less/variables";

.charge-manage {
    position: relative;
    width: 100%;
    .search-flag{
        width: 300px;
    };
    .search-name{
        margin-right: .5rem;
    };
    .search-input{
        width: 300px;
        margin: 0 15px;
    };
    .form-item {
        width: 100%;
        .item-label {
            min-width: 110px;
            text-align: right;
            vertical-align: middle;
            float: left;
            font-size: 14px;
            color: #606266;
            line-height: 40px;
            padding: 0 12px 0 0;
        }
    }
}
@charset "UTF-8";
@import "../../Common/less/variables";

.my-claim {
    min-height: 100%;
    position: relative;
    .no-claimed {
        padding-top: 2rem;
        .text {
            padding: 1rem 0;
            text-align: center;
            font-size: .8rem;
        }
        .el-icon-search {
            [class^=el-icon-] {
                color: #fff;
                font-weight: 700;
            }
        }
    }
    .my-img {
        img {
            position: absolute;
            left: 50%;
            top: 50%;
            margin-top: -63px;
            margin-left: -48px;
            width: 126px;
            height: 126px;
        }
    }
}

